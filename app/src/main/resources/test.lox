fun makeFilter() {
  fun filter(n) {
    if (n < 58) {
      return false;
    }
    return true;
  }
  return filter;
}

// This function applies a function to a list of
// numbers
fun applyToNumbers(f, count) {
  var n = 0;
  while (n < count) {
    if (f(n)) {
      print n;
    }
    n = n + 1;
  }
}

var greaterThanX = makeFilter();

print "Numbers >= 58:";
applyToNumbers(greaterThanX, 58 + 3);